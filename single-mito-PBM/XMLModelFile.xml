		<gpu:layerFunction>
			<name>Mito_Output</name>
		</gpu:layerFunction>
	</layer>
	<layer>
		<gpu:layerFunction>
            <name>Mito_CC</name>
        </gpu:layerFunction>
	</layer>
	<layer>
        <gpu:layerFunction>
            <name>Receptor_Output</name>
        </gpu:layerFunction>
    </layer>
    <layer>
    <gpu:layerFunction>
        <name>Receptor_Dissociate_Receptor</name>
    </gpu:layerFunction>
    </layer>
    <layer>
        <gpu:layerFunction>
            <name>Receptor_Bind_Receptor</name>
        </gpu:layerFunction>
    </layer>
    <layer>
        <gpu:layerFunction>
